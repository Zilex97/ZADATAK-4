			if (sNum.length > 1)
				var key2 = sNum[sNum.length - 2] + sNum[sNum.length - 1];
			if (key2 && pluralsConfig[key2]) {
				return pluralsConfig[key2];
			}
			else {
				return pluralsConfig[key1]
